{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    },
    "api/ws.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/ws",
      "dest": "/api/ws.js"
    },
    {
      "src": "/favicon.ico",
      "status": 204
    }
  ]
}